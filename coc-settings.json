{
    "clangd.semanticHighlighting": true,
    "suggest.triggerAfterInsertEnter": false,
    "suggest.timeout": 500,
    "suggest.noselect": true,
    "suggest.labelMaxLength": 30,
    "suggest.detailMaxLength": 30,
    "suggest.detailField": "abbr",
    "suggest.snippetIndicator": "",
    "suggest.triggerCompletionWait": 100,
    "suggest.echodocSupport": true,
    "suggest.completionItemKindLabels": {
        "keyword": "",
        "variable": "",
        "value": "",
        "operator": "",
        "function": "",
        "reference": "渚",
        "constant": "",
        "method": "",
        "struct": "פּ",
        "class": "",
        "interface": "",
        "text": "",
        "enum": "",
        "enumMember": "",
        "module": "",
        "color": "",
        "property": "",
        "field": "料",
        "unit": "",
        "event": "鬒",
        "file": "",
        "folder": "",
        "snippet": "",
        "typeParameter": "",
        "default": ""
    },
    "diagnostic.signOffset": 1,
    "coc.preferences.colorSupport": true,
    "explorer.icon.enableNerdfont": true,
    "explorer.icon.customIcons":{
      "icons": {},
      "extensions": {},
      "filenames": {},
      "dirnames": {},
      "patternMatches": {},
      "dirPatternMatches": {}
    },
    "explorer.file.column.indent.indentLine": false,
    "explorer.file.column.git.showIgnored": true,
    "explorer.file.column.git.icon.added": "+",
    "explorer.file.column.git.icon.untracked": "+",
    "explorer.file.column.git.icon.modified": "●",
    "explorer.file.column.git.icon.mixed": "",
    "explorer.keyMappings": {
        "i": false, // cancel default mapkey

        "gk": "", //"expandablePrev",
        "gj": "", //"expandableNext",

        "*": "toggleSelection",
        "<tab>": "actionMenu",

        "h": "",
        "l": "",  //["expandable?", "expand", "open"],
        "J": "",  //["toggleSelection", "nodeNext"],
        "K": "",  //["toggleSelection", "nodePrev"],
        "gl": "", //"expandRecursive",
        "gh": "", //"collapseRecursive",
        "<2-LeftMouse>": [
          "expandable?",
          ["expanded?", "collapse", "expand"],
          "open"
        ],
        "o": ["expanded?", "collapse", "expand"],
        "<cr>": [
          "expandable?",
          ["expanded?", "collapse", "expand"],
          "open"
        ],
        "e": "", //"open",
        "s": "", // "open:split",
        "S": "", // "open:split:plain",
        "E": "", // "open:vsplit",
        "t": "", // "open:tab",
        "<bs>": "gotoParent",
        "gp": "", //"preview:labeling",

        "y": "copyFilepath",
        "Y": "copyFilename",
        "c": "copyFile",
        "x": "cutFile",
        "p": "pasteFile",
        "d": "delete",
        "D": "deleteForever",

        "a": "addFile",
        "A": "addDirectory",
        "r": "rename",

        ".": "toggleHidden",
        "R": "refresh",

        "?": "help",
        "q": "quit",
        "<esc>": "esc",
        "X": "", //"systemExecute",
        "gd": "", //"listDrive",

        "f": "search",
        "F": "searchRecursive",

        "gf": "", //"gotoSource:file",
        "gb": "", //"gotoSource:buffer",

        "[[": "", //"sourcePrev",
        "]]": "", //"sourceNext",

        "[d": "", //"diagnosticPrev",
        "]d": "", //"diagnosticNext",

        "[c": "", //"gitPrev",
        "]c": "", //"gitNext",
        "<<": "", //"gitStage",
        ">>": ""  //"gitUnstage"
    },
    "texlab.path": "/usr/bin/texlab",
    "latex.forwardSearch.executable": "zathura",
    "latex.forwardSearch.args": ["--synctex-forward", "%l:1:%f", "%p"],
    "latex.build.onSave": true,

    "git.enableGutters": false,
    "git.signOffset": 99,
    "git.virtualTextPrefix": " ❯❯❯ ",
    "git.addGBlameToBufferVar": true,
    "git.addGBlameToVirtualText": true

    // language server
   //  "languageserver": {
